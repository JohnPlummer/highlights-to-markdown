{
  "name": "Highlights to Markdown",
  "author": "John Plummer",
  "description": "Add a 'Download Highlights' button to Instapaper articles",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["*://*.instapaper.com/read/*"],
      "js": ["contentScript.js"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "128": "/assets/icon-128.png"
  }
}
